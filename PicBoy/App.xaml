<Application x:Class="PicBoy.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="Forms/MainWindow.xaml">
    <Application.Resources>
        <Brush x:Key="FormBackground">#FF081105</Brush>
        <Color x:Key="FormBackgroundColor">#FF081105</Color>
        <Brush x:Key="WhiteBorderBrush">AntiqueWhite</Brush>
        <Brush x:Key="WhiteForeground">AntiqueWhite</Brush>
        <Color x:Key="WhiteColor">AntiqueWhite</Color>
        <Brush x:Key="GreenForeground">LimeGreen</Brush>
        <Brush x:Key="YellowForeground">YellowGreen</Brush>
        <Brush x:Key="EditForeground">Yellow</Brush>
        <Brush x:Key="RedForeground">OrangeRed</Brush>
    </Application.Resources>
</Application>